<h1>Schedule</h1>

<table>
  <thead>
  <tr>
    <th>Time</th>
    <th>Technician</th>
  </tr>
    <th>
      <% @technicians.each do |tech| %>
        <td><%= tech.name %></td>
      <% end %>
    </th>
  </thead>

  <tbody>
  <tr>
    <td>
      <% array = ["6:00am", "7:00am", "8:00am", "9:00am", "10:00am", "11:00am", "12:00pm", "13:00pm", "14:00pm", "15:00pm", "16:00pm", "17:00pm", "18:00pm"]
      array.each do |time| %>
        <tr>
          <td><%= time %></td>
        </tr>
      <% end %>
    </td>
    <td>
    <% @work_ord.each do |order| %>
      <tr>
        <td><%= order.time %></td>
        <td><%= order.duration %></td>
        <td><%= order.price %></td>
      </tr>
    </td>
    <% end %>
  </tbody>
</table>
    
